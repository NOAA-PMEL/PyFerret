use coads_climatology
list/form=cdf/ncformat=4/DEFLATE/file=a.nc/clobber/DATA=1 temp
 
! PLOT/DASH with /DATA=
plot/dash/x=180/z=0/DATASET=1 temp
 
! VECTOR/FLOW/DATA=/DENSITY
flow/DAT=2/den=3/L=3 taux,tauy
 
