sp ncdump -h a.nc | grep -i "`a,r=zaxis`"
 !-> sp ncdump -h a.nc | grep -i "PSZT"
	PSZT1_10 = 10 ;
	double PSZT1_10(PSZT1_10) ;
		PSZT1_10:units = "meters" ;
		PSZT1_10:positive = "down" ;
		PSZT1_10:standard_name = "depth" ;
