! Define a global variable
LET t30 = temp[Z=0:30@SUM]
 
! Another variable, setting title, units, bad flag.
LET/UNITS="Deg C"/BAD=100/TITLE="Indefinite integral of SALT" salty = salt[Z=@IIN]
 
 
