! Intentional errors
set mode ignore
 
! Name already in use
set axis/name=days2000 x_in_meters
 
! Missing or illegal name
set axis/name x_in_meters
set axis/name=45 x_in_meters
set axis/name=.fr x_in_meters
 
set mode/last ignore
