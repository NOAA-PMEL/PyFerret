! test for the behavior of bug 1336 by listing contents of variables;
 
let v1 = x[x=1:8]
let v2 = x[x=1:8] + y[y=1:3]
save/clobber/file=v12file.nc v1, v2
save/clobber/file=v1file.nc/i=3:8 v1
can var v1
can var v2
 
use v12file
use v1file
 
list v1  ! from vfile
             VARIABLE : X[X=1:8]
             FILENAME : v1file.nc
             SUBSET   : 6 points (X)
