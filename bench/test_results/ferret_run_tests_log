   <value><![CDATA[Y]]></value>
</attribute>
<attribute name="orig_file_axname" type="char">
   <value><![CDATA[grid_y_T]]></value>
</attribute>
</axis>
</axes>
!-> REPEAT: M:4
 !-> DEFINE SYMBOL m = 4
 !-> SH AXIS/XML zt
<axes>
<axis name="ZT">
<attribute name="direction" type="char">
   <value><![CDATA[K]]></value>
</attribute>
