! 1/00 additions to check details of @AVE bevavior
define axis/x=1.1:7.1:1 xoffset
define axis/x=5/npoints=1 x1pt
define axis/x=5:6:1 x2pt
 
! test 1D regrid
let a1 = x[gx=x1pt]	! single point
let a2 = x[gx=x2pt]
 
