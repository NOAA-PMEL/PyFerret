! Negative numbers cannot be listed with (i5.5) but can with (I05)
LET var1 =  FLOATSTR(sst-10,"(i5.5)")
LET var2 =  FLOATSTR(sst-10,"(i05)")
 
LIST/X=165W/L=1 var1[j=1:40:2], var2[j=1:40:2]
             DATA SET: ./coads_climatology.cdf
             LATITUDE: 91S to 11S
             LONGITUDE: 165W
             TIME: 16-JAN 06:00
