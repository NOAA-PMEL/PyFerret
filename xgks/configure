    fontdb/Makefile \
    doc/binding/Makefile \
    doc/userdoc/Makefile \
    doc/Makefile \
    progs/Makefile \
 port/udposix.h conftest*; exit 1' 1 3 15
PORT_MANIFEST='$PORT_MANIFEST'
